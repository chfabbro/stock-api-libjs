{
  "name": "stock-api-libjs",
  "engines": {
    "node": ">=6.3.1"
  },
  "version": "1.0.0",
  "private": false,
  "description": "Javascript implementation of the various APIs provided by the Stock services",
  "main": "src/index.js",
  "scripts": {
    "test": "node node_modules/karma/bin/karma start karma.conf.js --single-run && yarn run lint",
    "lint": "node node_modules/eslint/bin/eslint.js -f table ./ || true",
    "lint-jenkins": "node node_modules/eslint/bin/eslint.js -o tests/reports/eslint/log.xml -f junit ./",
    "build:prod": "export NODE_ENV=production|| set NODE_ENV=production&& webpack -p --colors --progress",
    "build:dev": "export NODE_ENV=development|| set NODE_ENV=development&& webpack --colors --progress",
    "build": "yarn run build:dev && yarn run build:prod",
    "watch": "export NODE_ENV=development|| set NODE_ENV=development&& webpack-dev-server --debug --progress --colors --hot"
  },
  "author": "Stock Web",
  "license": "Apache-2.0",
  "dependencies": {
    "requirejs": "2.3.6",
    "uuid": "3.3.2",
    "browserify-babel-istanbul": "^0.4.0",
    "isparta": "^4.0.0",
    "babelify": "^10.0.0"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-loader": "8.0.4",
    "babel-preset-es2015": "6.24.1",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "browserify": "^16.2.3",
    "browserify-istanbul": "^3.0.1",
    "chai": "^4.2.0",
    "eslint": "^5.9.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.2.0",
    "html-webpack-plugin": "^3.2.0",
    "istanbul": "^0.4.5",
    "jsdom": "13.0.0",
    "jsdom-global": "3.0.2",
    "karma": "^3.1.3",
    "karma-babel-preprocessor": "^7.0.0",
    "karma-browserify": "^6.0.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-firefox-launcher": "^1.0.1",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.3",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-safari-launcher": "^1.0.0",
    "karma-sinon-chai": "^2.0.2",
    "karma-sourcemap-loader": "^0.3.7",
    "mocha": "^5.2.0",
    "mocha-eslint": "^4.1.0",
    "npm": "^6.4.1",
    "nyc": "^13.1.0",
    "phantomjs-prebuilt": "^2.1.14",
    "sinon": "7.1.1",
    "sinon-chai": "^3.3.0",
    "webpack": "4.27.1",
    "webpack-dev-server": "^3.1.10"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/adobe/stock-api-libjs.git"
  }
}
